{
  "name": "check-markup-language",
  "version": "1.0.0",
  "private": true,
  "description": "A markup language for designing tests",
  "scripts": {
    "dev": "vite-node --config vite.config.ts src/index.ts",
    "prod": "node dist/index.js",
    "start": "start dist/index.exe",
    "build": "tsc && vite build",
    "postbuild": "pkg --compress GZip --target node18-win-x64 --output dist/index.exe src/index.ts",
    "preview": "vite preview",
    "test": "vitest",
    "test:ui": "vitest --ui"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/CedricDeVon/Check-Markup-Language.git"
  },
  "keywords": [
    ""
  ],
  "author": "Vaughn Cedric L. Araneta",
  "license": "ISC",
  "type": "commonjs",
  "bugs": {
    "url": "https://github.com/CedricDeVon/Check-Markup-Language/issues"
  },
  "homepage": "https://github.com/CedricDeVon/Check-Markup-Language#readme",
  "devDependencies": {
    "@rollup/plugin-commonjs": "^28.0.2",
    "@types/cli": "^0.11.25",
    "@types/cli-progress": "^3.11.6",
    "@types/cli-spinner": "^0.2.3",
    "@types/commander": "^2.12.0",
    "@types/dotenv": "^6.1.1",
    "@types/exceljs": "^0.5.3",
    "@types/figlet": "^1.7.0",
    "@types/node": "^22.10.5",
    "@types/node-cache": "^4.1.3",
    "@types/validator": "^13.12.2",
    "@types/winston": "^2.4.4",
    "@vitest/ui": "^2.1.8",
    "typescript": "^5.7.2",
    "vite": "^6.0.7",
    "vite-node": "^2.1.8",
    "vitest": "^2.1.8"
  },
  "dependencies": {
    "cli": "^1.0.1",
    "cli-progress": "^3.12.0",
    "cli-spinner": "^0.2.10",
    "commander": "^13.0.0",
    "dotenv": "^16.4.7",
    "exceljs": "^4.4.0",
    "figlet": "^1.8.0",
    "node-cache": "^5.1.2",
    "pkg": "^5.8.1",
    "supports-hyperlinks": "^3.1.0",
    "validator": "^13.12.0",
    "winston": "^3.17.0"
